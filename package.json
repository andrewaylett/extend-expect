{
  "name": "extend-expect",
  "description": "Types and a helper for extending Jest's expect",
  "author": "Andrew Aylett",
  "version": "1.1.0",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/andrewaylett/extend-expect.git"
  },
  "engines": {
    "node": ">=18"
  },
  "type": "module",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run buildonly",
    "buildonly": "tsc -b --verbose .",
    "test": "echo 'tested in post-build'",
    "postbuild": "NODE_OPTIONS=--experimental-vm-modules jest test/postbuild.test.ts",
    "lint": "eslint .",
    "prebuild": "npm run test",
    "pretest": "npm run lint",
    "lint:fix": "npm run lint -- --fix",
    "dev": "npm run lint:fix && npm run buildonly && npm run test",
    "prepublishOnly": "npm run build"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "devDependencies": {
    "@jest/globals": "29.7.0",
    "@tsconfig/node18": "18.2.2",
    "@types/jest": "29.5.5",
    "@types/node": "18.17.17",
    "@types/semver": "7.5.2",
    "eslint": "8.49.0",
    "eslint-config-andrewaylett": "3.0.0",
    "eslint-plugin-jest": "27.4.0",
    "expect": "29.7.0",
    "jest": "29.7.0",
    "prettier": "3.0.3",
    "semver": "7.5.4",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "5.2.2"
  },
  "peerDependencies": {
    "@jest/globals": "^28 || ^29",
    "expect": "^28 || ^29",
    "jest-matcher-utils": "^28 || ^29"
  }
}
